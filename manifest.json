{
  "name": "Nomnoml Extension",
  "version": "1.0.0",
  "description": "Render nomnoml diagrams in markdown codeblocks",
  "manifest_version": 2,
  "permissions": [
     "tabs", "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["lib/dagre.js", "lib/nomnoml.js", "index.js"]
    }
  ]
}